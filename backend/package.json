{
  "name": "intune-backend",
  "version": "1.0.0",
  "private": true,
  "description": "Express API for the Intune portfolio app",
  "type": "commonjs",
  "main": "server.js",
  "engines": {
    "node": ">=18.18.0"
  },
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "pg": "^8.13.1",
    "rate-limiter-flexible": "^7.2.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "supertest": "^7.1.0"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>"
    ],
    "testMatch": [
      "**/*.test.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "testTimeout": 10000
  },
  "license": "ISC"
}
